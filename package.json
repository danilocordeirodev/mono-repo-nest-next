{
  "name": "@full-stack/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "env-cmd -f .local.env concurrently --kill-others \"npm:db:up\" \"npm:dev:web\" \"npm:dev:api\"",
    "dev:api": "nx serve api --verbose",
    "dev:web": "nx dev web --verbose",
    "test": "nx test",
    "db:up": "npm run db:docker && npm run db:dev-migrate && npm run db:create-client -- --watch",
    "db:docker": "docker compose -f tools/docker-env/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:create-client": "prisma generate",
    "db:dev-migrate": "env-cmd --no-override -f .local.env npx prisma migrate dev",
    "db:studio": "env-cmd --no-override -f .local.env npx prisma studio",
    "gen:gql": "graphql-codegen --config tools/gql-codegen/gql-codegen.yml --watch"
  },
  "prisma": {
    "schema": "libs/api/data-access-db/src/lib/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@mercuriusjs/gateway": "^1.2.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/mercurius": "^12.1.1",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/platform-fastify": "^10.3.8",
    "@parcel/watcher": "^2.4.1",
    "@prisma/client": "^5.13.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.1.0",
    "mercurius": "^13.4.1",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "urql": "^4.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.6",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "18.3.4",
    "@nx/eslint-plugin": "18.3.4",
    "@nx/jest": "18.3.4",
    "@nx/js": "18.3.4",
    "@nx/nest": "18.3.4",
    "@nx/next": "^18.3.4",
    "@nx/node": "18.3.4",
    "@nx/web": "18.3.4",
    "@nx/webpack": "18.3.4",
    "@nx/workspace": "18.3.4",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "babel-jest": "^29.4.1",
    "concurrently": "^8.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "~8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "graphql-codegen": "^0.4.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "18.3.4",
    "prettier": "^2.6.2",
    "prisma": "^5.13.0",
    "prisma-nestjs-graphql": "^20.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2",
    "webpack-cli": "^5.1.4"
  }
}
